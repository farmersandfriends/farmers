    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="com.android.vending.BILLING" />

    <uses-feature android:name="android.hardware.location.gps" />

    <application
        android:name=".loginandregistration.app.AppController"
        android:debuggable="false"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".LogoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".loginandregistration.activity.LoginActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustPan"></activity>
        <activity
            android:name=".loginandregistration.activity.RegisterActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".MaintoOther.MyUnit" />
        <activity android:name=".MaintoOther.Like" />
        <activity android:name=".MaintoOther.Information" />
        <activity android:name=".MaintoOther.Turn" />
        <activity android:name=".MaintoOther.TheOthers" />
        <activity android:name=".MaintoOther.information.term" />
        <activity
            android:name=".MaintoOther.information.showterm"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".MaintoOther.information.technic" />
        <activity android:name=".MaintoOther.information.video" />
        <activity android:name=".MaintoOther.information.adapter.Webview" />
        <activity android:name=".MaintoOther.diary.diary" />
        <activity android:name=".MaintoOther.vacant.vacants" />
        <activity
            android:name=".MaintoOther.Forecast.TodayWeather"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity android:name=".MaintoOther.Forecast.Weather" />
        <activity
            android:name=".Drawer"
            android:label="@string/title_activity_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".loginandregistration.activity.Board"
            android:label="@string/app_name"
            android:launchMode="singleTop" />
        <activity
            android:name=".loginandregistration.activity.DialogActivity"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".loginandregistration.activity.SettingActivity" />
        <activity
            android:name=".MaintoOther.vacant.showvacant"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".MaintoOther.vacant.field" />
        <activity android:name=".MaintoOther.billing.billingactivity"></activity>
        <activity android:name=".loginandregistration.activity.setting.ImgUpload" />
    </application>

